{
    "name" : "stefanotorresi/my-base",
    "description" : "A general purpose ZF2 module that provides miscellaneous classes",
    "type" : "library",
    "require" : {
        "php" : ">=5.4.1"
    },
    "require-dev": {
        "kriswallsmith/assetic": "1.*",
        "soflomo/purifier": "0.*",
        "zendframework/zendframework" : "2.*",
        "doctrine/orm": "2.*",
        "doctrine/doctrine-orm-module": "~0.8"
    },
    "suggest": {
        "doctrine/orm" : "Doctrine ORM is required to use MyBase\\Doctrine package",
        "kriswallsmith/assetic": "",
        "soflomo/purifier": ""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-0.x": "0.x-dev"
        }
    },
    "autoload" : {
        "psr-4": {
            "MyBase\\": "src/",
            "MyBase\\Test\\": "tests/Test"
        },
        "classmap": [
            "./"
        ]
    },
    "license": "MIT"
}
